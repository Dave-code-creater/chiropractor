{
  "info": {"name": "Report Service", "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"},
  "variable": [{"key": "baseUrl", "value": "http://localhost:3003"}],
  "item": [
    {"name": "Health", "request": {"method": "GET", "url": "{{baseUrl}}/"}},
    {
      "name": "Create Report",
      "request": {
        "method": "POST",
        "header": [{"key": "Content-Type", "value": "application/json"}],
        "url": "{{baseUrl}}/reports",
        "body": {"mode": "raw", "raw": "{\n  \"owner_id\": 1,\n  \"data\": {}\n}"}
      }
    },
    {"name": "List Reports", "request": {"method": "GET", "url": "{{baseUrl}}/reports"}},
    {"name": "Get Report", "request": {"method": "GET", "url": "{{baseUrl}}/reports/1"}},
    {
      "name": "Update Report",
      "request": {
        "method": "PUT",
        "header": [{"key": "Content-Type", "value": "application/json"}],
        "url": "{{baseUrl}}/reports/1",
        "body": {"mode": "raw", "raw": "{\n  \"data\": {}\n}"}
      }
    }
  ]
}
