{
  "name": "chiropractor-monolith",
  "version": "1.0.0",
  "description": "Dr. Dieu Phan D.C - Chiropractic Practice Management System (Monolithic)",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "NODE_ENV=test mocha test/**/*.test.js",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "add-doctor": "node scripts/add-doctor-dieu-phan.js",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "docker:build": "docker build -t chiropractor-monolith .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:clean": "docker-compose down -v --remove-orphans"
  },
  "dependencies": {
    "axios": "^1.12.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^8.1.0",
    "http-status-codes": "^2.3.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.27.3",
    "morgan": "^1.10.0",
    "opossum": "^9.0.0",
    "pg": "^8.11.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ua-parser-js": "^2.0.3",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "chai": "^5.2.0",
    "chai-spies": "^1.1.0",
    "eslint": "^8.55.0",
    "mocha": "^10.4.0",
    "nodemon": "^3.0.2",
    "sinon": "^17.0.1",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "chiropractic",
    "practice-management",
    "healthcare",
    "monolith",
    "nodejs",
    "postgresql"
  ],
  "author": "Dr. Dieu Phan D.C",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/chiropractor.git"
  }
}
